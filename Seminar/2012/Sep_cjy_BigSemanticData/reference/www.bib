@INPROCEEDINGS{Kroetzsch2011,
  author = {Markus Krötzsch and Frederick Maier and Adila A. Krisnadhi and Pascal
	Hitzler},
  title = {A Better Uncle for OWL: Nominal Schemas for Integrating Rules and
	Ontologies},
  booktitle = {20th International World Wide Web Conference (WWW2011)},
  year = {2011},
  pages = {645},
  month = {March/April},
  abstract = {We propose a description-logic style extension of OWL 2 with nominal
	schemas which can be used like "variable nominal classes" within
	axioms. This feature allows ontology languages to express arbitrary
	DL-safe rules (as expressible in SWRL or RIF) in their native syntax.
	We show that adding nominal schemas to OWL 2 does not increase the
	worst-case reasoning complexity, and we identify a novel tractable
	language SROELV_3 that is versatile enough to capture the lightweight
	languages OWL EL and OWL RL.},
