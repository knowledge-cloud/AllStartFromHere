@INPROCEEDINGS{Papailiou2012,
  author = {Nikolaos Papailiou and Ioannis Konstantinou and Dimitrios Tsoumakos
	and Nectarios Koziris},
  title = {H2RDF: Adaptive Query Processing on RDF Data in the Cloud},
  booktitle = {21st International World Wide Web Conference (WWW2012)},
  year = {2012},
  abstract = {In this work we present H2RDF, a fully distributed RDFstore that combines
	the MapReduce processing frameworkwith a NoSQL distributed data store.
	Our system featurestwo unique characteristics that enable efficient
	processingof both simple and multi-join SPARQL queries on virtuallyunlimited
	number of triples: A query planner that enablesadaptive decisions
	over both the join order as well as thejoin execution; and join algorithms
	that execute joins according to query selectivity to reduce processing.
	We haveevaluated H2RDF against state-of-the-art RDF stores: Oursystem
	achieves the best of both worlds, answering simplejoins as fast as
	centralized stores and greatly outperformingboth distributed and
	centralized schemes for large, multiple-join queries. In this demonstration
	we showcase the system'sfunctionality through an interactive GUI.
	Users will be ableto execute predefined or custom-made SPARQL queries
	ondatasets of different sizes, using different join algorithms.Moreover,
	they can repeat all queries utilizing a differentnumber of cluster
	resources. Using real-time cluster monitoring and detailed statistics,
	participants will be able tounderstand the advantages of different
	execution schemesversus the input data as well as the scalability
	properties ofH2RDF over both the data size and the available workerresources.},
  file = {:/home/jychen/Documents/BigSemanticData/WWW/H2RDF\: Adaptive Query Processing on RDF Data in the Cloud.pdf:PDF},
  url = {http://data.semanticweb.org/conference/www/2012/demo/36}
}

